<link rel="stylesheet" href="/css/blog-detail.css">
<main class="blog-detail">
    <h1><%= blog.title %></h1>
    <p class="meta">
        By <%= blog.author %> |
        <%= blog.created_at.toLocaleDateString("en-US", { month: "long", day: "numeric", year: "numeric" }) %>
    </p>
    <div class="content">
        <%= blog.content %>
    </div>
    <a href="/<%= returnTo %>" class="back">‚Üê Back to Blogs</a>
</main>
<script src="/js/script.js"></script>